# YouTube API Credentials
GOOGLE_OAUTH_CLIENT_ID=your_client_id_here
GOOGLE_OAUTH_CLIENT_SECRET=your_client_secret_here
GOOGLE_OAUTH_REDIRECT_URI=http://localhost

# Path to yt-dlp script (if not in PATH)
YTDLP_PATH=/path/to/yt-dlp

# YouTube Playlist IDs
PLAYLIST_CITY_COMMISSION=your_city_commission_playlist_id
PLAYLIST_GENERAL_POLICY=your_general_policy_playlist_id
PLAYLIST_CITY_PLAN_BOARD=your_city_plan_board_playlist_id
PLAYLIST_UTILITY_ADVISORY_BOARD=

# Cleanup Configuration
CLEANUP_AFTER_DAYS=30
# DRY_RUN=true

# Redis Configuration (for queue)
REDIS_HOST=localhost
REDIS_PORT=6379

# GPU Configuration (for diarization)
GPU_DEVICE=0
HF_TOKEN=your_huggingface_token_here

FILE_SERVER_PORT=3000
