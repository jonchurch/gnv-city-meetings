# YouTube OAuth credentials
# Create a project at https://console.cloud.google.com/
# Then enable the YouTube Data API v3 and create OAuth 2.0 credentials

# Required: Client ID from Google Cloud Console
GOOGLE_OAUTH_CLIENT_ID=your-client-id.apps.googleusercontent.com

# Required: Client Secret from Google Cloud Console
GOOGLE_OAUTH_CLIENT_SECRET=your-client-secret

# Optional: Custom redirect URI (defaults to http://localhost)
GOOGLE_OAUTH_REDIRECT_URI=http://localhost
